.gemstones-list .content {
  position: relative;
  width: 90%;
  overflow: hidden;
  padding:10px;
}

.gemstones-list .content .content-overlay {
  background: rgba(0,0,0,0.7);
  position: absolute;
  height: 80%;
  width: 80%;
  left: 10%;
  top: 10%;
  bottom: 0;
  right: 0;
  opacity: 0;
  -webkit-transition: all 0.4s ease-in-out 0s;
  -moz-transition: all 0.4s ease-in-out 0s;
  transition: all 0.4s ease-in-out 0s;
}

.gemstones-list .content:hover .content-overlay{
  opacity: 1;
}

.gemstones-list .content-image{
  width: 100%;
  padding: 2px;
   border: 1px solid #ededed;
}

.gemstones-list .content-details {
  position: absolute;
  text-align: center;
  padding-left: 1em;
  padding-right: 1em;
  width: 100%;
  top: 50%;
  left: 50%;
  opacity: 0;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-transition: all 0.3s ease-in-out 0s;
  -moz-transition: all 0.3s ease-in-out 0s;
  transition: all 0.3s ease-in-out 0s;
}

.gemstones-list .content:hover .content-details{
  top: 50%;
  left: 50%;
  opacity: 1;
}

.gemstones-list .content-details h3{
  color: #fff;
  font-weight: 500;
  letter-spacing: 0.15em;
  margin-bottom: 0.5em;
  text-transform: uppercase;
}

.gemstones-list .content-details p{
  color: #fff;
  font-size: 0.8em;
  padding-left: 7%;
  padding-right: 7%;
}



.gemstones-list .fadeIn-top{
  top: 20%;
}

.gemstones-list .fadeIn-right{
  left: 80%;
}


/****************** mega menu ********************/


.top_navigation ul{
  list-style:none;
  margin: 0;
  padding: 0;
}


.top_navigation .exo-menu{
  width: 100%;
  float: left;
  list-style: none;
  position:relative;
}
.top_navigation .exo-menu > li {  display: inline-block;float:left;}
.top_navigation .exo-menu > li > a{
  color: #000;
  text-decoration: none;
  text-transform: uppercase;
  border-right: none;
  -webkit-transition: color 0.2s linear, background 0.2s linear;
  -moz-transition: color 0.2s linear, background 0.2s linear;
  -o-transition: color 0.2s linear, background 0.2s linear;
  transition: color 0.2s linear, background 0.2s linear;
}
.top_navigation .exo-menu > li > a.active,
.top_navigation .exo-menu > li > a:hover,
.top_navigation li.drop-down ul > li > a:hover{
  background:#009FE1;
  color:#fff;
}
.top_navigation .exo-menu i {
  float: left;
  font-size: 18px;
  margin-right: 6px;
  line-height: 20px !important;
}
.top_navigation li.drop-down,
.top_navigation .flyout-right,
.top_navigation .flyout-left{position:relative;}
/*.top_navigation li.drop-down:before {
  content: "\f103";
  color: #fff;
  font-family: FontAwesome;
  font-style: normal;
  display: inline;
  position: absolute;
  right: 6px;
  top: 20px;
  font-size: 14px;
}*/
.top_navigation li.drop-down>ul{
  left: 0px;
  min-width: 230px;

}
.top_navigation .drop-down-ul{display:none;}
.top_navigation .flyout-right>ul,
.top_navigation .flyout-left>ul{
  top: 0;
  min-width: 230px;
  display: none;
  border-left: 1px solid #365670;
  }

.top_navigation li.drop-down>ul>li>a,
.top_navigation .flyout-right ul>li>a ,
.top_navigation .flyout-left ul>li>a {
  color: #fff;
  display: block;
  padding: 15px 22px;
  text-decoration: none;
  background-color: #fff;
  border-bottom: 1px dotted #547787;
  -webkit-transition: color 0.2s linear, background 0.2s linear;
  -moz-transition: color 0.2s linear, background 0.2s linear;
  -o-transition: color 0.2s linear, background 0.2s linear;
  transition: color 0.2s linear, background 0.2s linear;
  color: #000;
font-size: 12px;
}

.top_navigation .exo-menu li.drop-down ul.drop-down-ul{border: 1px solid #ededed;border-left: 1px solid;}
.top_navigation .flyout-right ul>li>a ,
.top_navigation .flyout-left ul>li>a {
  border-bottom: 1px dotted #B8C7BC;
}


/*Flyout Mega*/
.top_navigation .flyout-mega-wrap {
  top: 0;
  right: 0;
  left: 100%;
  width: 100%;
  display:none;
  height: 100%;
  padding: 15px;
  min-width: 742px;

}
.top_navigation h4.row.mega-title {
  color:#eee;
  margin-top: 0px;
  font-size: 14px;
  padding-left: 15px;
  padding-bottom: 13px;
  text-transform: uppercase;
  border-bottom: 1px solid #ccc;
 }
.top_navigation .flyout-mega ul > li > a {
  font-size: 90%;
  line-height: 25px;
  color: #fff;
  font-family: inherit;
}
.top_navigation .flyout-mega ul > li > a:hover,
.top_navigation .flyout-mega ul > li > a:active,
.top_navigation .flyout-mega ul > li > a:focus{
  text-decoration: none;
  background-color: transparent !important;
  color: #ccc !important
}
/*mega menu*/

.top_navigation .mega-menu {
  left: 0;
  right: 0;
  padding: 15px;
  display:none;
  padding-top: 0;
  min-height: 100%;
  border: border: 1px solid rgba(0, 0, 0, 0.3);;
 /* margin-top: 12px; */

}
.top_navigation h4.row.mega-title {
  color: #000;
  margin-top: 0px;
  font-size: 11px;
  padding-left: 15px;
  padding-bottom: 5px;
  text-transform: uppercase;
  border-bottom: none;
  padding-top: 15px;
  background-color: #fff;
  font-weight: 600;
  letter-spacing: 0.7px;
  }
.top_navigation  .mega-menu ul li a {
  line-height: 25px;
  font-size: 90%;
  display: block;
}
.top_navigation ul.stander li a {
    padding: 3px 0px;
}

.top_navigation ul.description li {
    padding-bottom: 12px;
    line-height: 8px;
}

.top_navigation ul.description li span {
    color: #ccc;
    font-size: 85%;
}
.top_navigation a.view-more{
  border-radius: 1px;
  margin-top:15px;
  background-color: #009FE1;
  padding: 2px 10px !important;
  line-height: 21px !important;
  display: inline-block !important;
}
.top_navigation a.view-more:hover{
  color:#fff;
  background:#0DADEF;
}
.top_navigation ul.icon-des li a i {
    color: #fff;
    width: 35px;
    height: 35px;
    border-radius: 50%;
    text-align: center;
    background-color: #009FE1;
    line-height: 35px !important;
}

.top_navigation ul.icon-des li {
    width: 100%;
    display: table;
    margin-bottom: 11px;
}

/*common*/
.top_navigation .flyout-right ul>li>a ,
.top_navigation .flyout-left ul>li>a,
.top_navigation .flyout-mega-wrap,
.top_navigation .mega-menu{
  background-color: #fff;
}

/*hover*/

.top_navigation .mega-menu:hover,
.top_navigation .drop-down-ul:hover,
.top_navigation li.flyout-left>ul:hover,
.top_navigation li.flyout-right>ul:hover,
.top_navigation .flyout-mega-wrap:hover,
.top_navigation li.flyout-left a:hover +ul,
.top_navigation li.flyout-right a:hover +ul,
.top_navigation .blog-drop-down >a:hover+.Blog,
.top_navigation li.drop-down>a:hover +.drop-down-ul,
.top_navigation .images-drop-down>a:hover +.Images,
.top_navigation .mega-drop-down a:hover+.mega-menu,
.top_navigation li.flyout-mega>a:hover +.flyout-mega-wrap{
  display:block;
}
/*responsive*/
 @media (min-width:767px){
.top_navigation   .exo-menu > li > a{
  display:block;
  padding: 8px 22px;
 }
.top_navigation .mega-menu,.top_navigation  .flyout-mega-wrap,.top_navigation .flyout-right>ul,
.top_navigation .flyout-left>ul,.top_navigation  li.drop-down>ul{
    position:absolute;
}
.top_navigation  .flyout-right>ul{
  left: 100%;
  }
.top_navigation   .flyout-left>ul{
  right: 100%;
}
 }
@media (max-width:767px){

 .top_navigation  .exo-menu {
    min-height: 58px;
    background-color: #23364B;
    width: 100%;
  }
  
.top_navigation   .exo-menu > li > a{
    width:100% ;
      display:none ;
  
  }
.top_navigation   .exo-menu > li{
    width:100%;
  }
 .top_navigation  .display.exo-menu > li > a{
    display:block ;
      padding: 20px 22px;
  }
  
.top_navigation .mega-menu,.top_navigation .flyout-right>ul,
.top_navigation .flyout-left>ul,.top_navigation  li.drop-down>ul{
    position:relative;
}

}


.top_navigation .megamenusip,.top_navigation  .mega-menu,.top_navigation .flyout-right>ul,
.top_navigation .flyout-left>ul,.top_navigation  li.drop-down>ul{
  z-index: 2000;
}
.top_navigation .exo-menu > li > a.active,.top_navigation  .exo-menu > li > a:hover,.top_navigation  li.drop-down ul > li > a:hover {
    background-color: #fff !important;
	color: var(--custom-color) !important;
}
